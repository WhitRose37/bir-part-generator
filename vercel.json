{
  "version": 2,
  "buildCommand": "prisma generate && npm run build",
  "installCommand": "npm ci && npx prisma generate",
  "framework": "nextjs",
  "regions": ["sin1"],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "build": {
    "env": {
      "PRISMA_CLI_BINARY_TARGETS": "native,rhel-openssl-3.0.x"
    }
  }
}
